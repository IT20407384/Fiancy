<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daemon.fiancy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FIANCY">

        <activity android:name=".MainActivity"
            android:noHistory="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Reject_Reason" />
        <activity android:name=".ProfileReview" />
        <activity android:name=".Search" />
        <activity android:name=".ReportAd" />
        <activity android:name=".profile" />
        <activity android:name=".Home" />
        <activity android:name=".MatchFinder" />
        <activity android:name=".PaypalUI" />
        <activity android:name=".AdConfirmActivity" />
        <activity android:name=".PostAdActivity" />
        <activity android:name=".UpdateProfile" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".PremiumLog" />
        <activity android:name=".AdminPanel" />
        <activity android:name=".MyAdCollection" />
        <activity android:name=".ManageAdvertisementActiviy"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>